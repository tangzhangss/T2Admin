{
	"table": {
		"self": {
			"style": {
			"border-collapse":"collapse",
			"border-spacing":"0",
			"margin":"0px auto",
			"background-color":"#fff",
			"color":"#666"
			},
			"pseudoClass": {
				"hover": {},
				"active": {}
			}
		},
		"clild": {
			"thead": {
				"clild":{
					"tr": {
						"self": {
							"style": {
							"background-color":"#f2f2f2",
							"transition":"all .3s",
							"-webkit-transition":"all .3s"
							},
							"pseudoClass": {
								"hover": {"background-color": "#f2f2f2"},
								"active": {},
								"odd": {},
								"even": {}
							}
						},
						"clild": {
							"td": {
								"self": {
									"style": {
									"text-align":"left",
									"font-weight":"400",
									"border":"1px solid #e6e6e6 !important",
									"padding":"9px 15px",
									"line-height":"20px",
									"font-size":"14px"
									}
								}
							}
						}
					}
				}
			},
			"tbody": {
				"clild":{
					"tr": {
					"self": {
						"style": {
							"transition":"all .3s",
							"-webkit-transition":"all .3s"
						},
						"pseudoClass": {
							"hover": {"background-color": "#f2f2f2"},
							"active": {},
							"odd": {},
							"even": {}
						}
					},
					"clild": {
						"td": {
							"self": {
								"style": {
									"text-align":"left",
									"font-weight":"400",
									"border":"1px solid #e6e6e6",
									"padding":"9px 15px",
									"min-height":"20px",
									"line-height":"20px",
									"font-size":"14px"
								},
								"pseudoClass": {
									"hover": {},
									"active": {}
								}
							},
							"clild": {}
						}
					}
				}
				}
			},
			"tfoot": {

			}
		}
	}
}